@Base
--track0:Shift,-500,500,0
--track1:Mode,0,1,0,1
--track2:z-value,-8000,8000,1024
--check0:Support Camera,0
--dialog:BaseColor/col,local col=0x5588ff;MovePos/chk,local mv=0;Coordinate,pos={-200,-100,0, 0,0,0};Show Coordinate/chk,local view=0;ChildBase/chk,local ko=0;LightExtent%,local limit=100;

  if mv==0 then
    obj.setanchor("pos",2,"line","xyz")
    if not obj.check0 then
      CustomFlareXX=pos[1]
      CustomFlareYY=pos[2]
      CustomFlareZZ=pos[3]
      CustomFlareCX=pos[4]
      CustomFlareCY=pos[5]
      CustomFlareCZ=pos[6]
      cdd=1
      palpha=1
      oncamera=0
    else
      if obj.getoption("camera_mode")~=0 then
        CustomFlareW,CustomFlareH=obj.getpixel()
        CustomFlareMode=1+3*obj.track1
        obj.copybuffer("tmp","obj")
        obj.setoption("drawtarget","tempbuffer")
        obj.setoption("blend",CustomFlareMode)
        obj.setfont("",50,4,0xffffff,0)
        obj.load("Please deactivate camera mode on top left")
        obj.draw(0,0)
        obj.load("tempbuffer")
        obj.setoption("blend",0)
        CustomFlareXX=pos[1]
        CustomFlareYY=pos[2]
        CustomFlareZZ=pos[3]
        CustomFlareCX=pos[4]
        CustomFlareCY=pos[5]
        CustomFlareCZ=pos[6]
        cdd=1
        palpa=1
        oncamera=0
      else
        oncamera=1
        if ko==nil then
          ko=0
        end
	local cam=obj.getoption("camera_param")
        if ko==0 then
          CustomFlare_default_z=obj.track2
        end
        tr3=math.abs((cam.d+CustomFlare_default_z)/cam.d)
        if ko==0 then
          flarez=pos[3]+CustomFlare_default_z+obj.getvalue("z")
          flarex=(pos[1]+obj.getvalue("x"))*tr3--/(obj.track3*0.01)
          flarey=(pos[2]+obj.getvalue("y"))*tr3--/(obj.track3*0.01)
        end
        local cx=cam.x
        local cy=cam.y
        local cz=cam.z
        local ex=cam.tx-cx
        local ey=cam.ty-cy
        local ez=cam.tz-cz
        local er=(ex*ex+ey*ey+ez*ez)^0.5
        if er==0 then
          ex=0
          ey=0
          ez=1
        else
          ex=ex/er
          ey=ey/er
          ez=ez/er
        end
        local ux=cam.ux
        local uy=cam.uy
        local uz=cam.uz
        er=(ux*ux+uy*uy+uz*uz)^0.5
        if er==0 then
          ux=0
          uy=-1
          uz=0
        else
          ux=ux/er
          uy=uy/er
          uz=uz/er
        end
        local uxx=uy*ez-uz*ey
        local uxy=uz*ex-ux*ez
        local uxz=ux*ey-uy*ex
        er=(uxx*uxx+uxy*uxy+uxz*uxz)^0.5
        if er==0 then
          uxx=-1
          uxy=0
          uxz=0
        else
          uxx=uxx/er
          uxy=uxy/er
          uxz=uxz/er
        end
        local uyx=ey*uxz-ez*uxy
        local uyy=ez*uxx-ex*uxz
        local uyz=ex*uxy-ey*uxx
        er=(uyx*uyx+uyy*uyy+uyz*uyz)^0.5
        if er==0 then
          uyx=-1
          uyy=0
          uyz=0
        else
          uyx=uyx/er
          uyy=uyy/er
          uyz=uyz/er
        end
        d=(flarex-cx)*ex+(flarey-cy)*ey+(flarez-cz)*ez
        local dx=cx+ex*d
        local dy=cy+ey*d
        local dz=cz+ez*d
        local dox=flarex-dx
        local doy=flarey-dy
        local doz=flarez-dz
        local dr=(dox*dox+doy*doy+doz*doz)^0.5
        if dr==0 then
          dox=0
          doy=0
          doz=1
        else
          dox=dox/dr
          doy=doy/dr
          doz=doz/dr
        end
        if uxx*dox+uxy*doy+uxz*doz<=0 then
          p=1
        else
          p=-1
        end
        local dc=dox*uyx+doy*uyy+doz*uyz
        local ds=1-dc*dc
        if ds<0 then
          ds=0
        end
        ds=ds^0.5
        local dx=p*dr*ds
        local dy=-dr*dc
        cd=cam.d
        if d<=0 then
          palpha=0
        else
          palpha=1
        end
        cdd=cd/d
        local rcos=math.cos(cam.rz*math.pi/180)*cdd
        local rsin=-math.sin(cam.rz*math.pi/180)*cdd
        cdd=cd/d*tr3--/(obj.track3*0.01)
        local px=dx*rcos+dy*rsin
        local py=-dx*rsin+dy*rcos
        if tr3<0 then
          px=-px
          py=-py
        end
        CustomFlareXX=px--pos[1]*dcd
        CustomFlareYY=py--pos[2]*dcd
        CustomFlareZZ=0--pos[3]*dcd
        if ko==0 then
          CustomFlareCX=pos[4]
          CustomFlareCY=pos[5]
          CustomFlareCZ=pos[6]
        end
        if view==1 then
          CustomFlareW,CustomFlareH=obj.getpixel()
          CustomFlareMode=1+3*obj.track1
          obj.copybuffer("tmp","obj")
          obj.setoption("drawtarget","tempbuffer")
          obj.setoption("blend",CustomFlareMode)
          obj.setfont("",50,4,0xffffff,0)
          obj.load("X:"..flarex)
          obj.draw(0,-50)
          obj.load("Y:"..flarey)
          obj.draw(0,0)
          obj.load("Z:"..flarez)
          obj.draw(0,50)
          obj.load("tempbuffer")
          obj.setoption("blend",0)
        end
      end
    end
  else
    oncamera=0
    obj.setanchor("pos",4,"line","xyz","inout")
    local s=obj.track0*0.01
    CustomFlareXX=(1-s)*pos[1]+s*pos[7]
    CustomFlareYY=(1-s)*pos[2]+s*pos[8]
    CustomFlareZZ=(1-s)*pos[3]+s*pos[9]
    CustomFlareCX=(1-s)*pos[4]+s*pos[10]
    CustomFlareCY=(1-s)*pos[5]+s*pos[11]
    CustomFlareCZ=(1-s)*pos[6]+s*pos[12]
    cdd=1
    palpha=1
    if obj.check0 then
      CustomFlareW,CustomFlareH=obj.getpixel()
      CustomFlareMode=1+3*obj.track1
      obj.copybuffer("tmp","obj")
      obj.setoption("drawtarget","tempbuffer")
      obj.setoption("blend",CustomFlareMode)
      obj.setfont("",50,4,0xffffff,0)
      obj.load("Please uncheck MovePos")
      obj.draw(0,-50)
      obj.load("For Camera Support")
      obj.draw(0,0)
    end
  end

  CustomFlaredX = (CustomFlareCX-CustomFlareXX)
  CustomFlaredY = (CustomFlareCY-CustomFlareYY)
  CustomFlaredZ = (CustomFlareCZ-CustomFlareZZ)
  if obj.check0 and mv==0 then
    if limit~=nil then
      if 0<=limit then
        limit=limit*0.01
        CustomFlaredX=CustomFlaredX*limit
        CustomFlaredY=CustomFlaredY*limit
        CustomFlaredZ=CustomFlaredZ*limit
      else
        local er=(CustomFlaerdX*CustomFlaerdX+CustomFlaerdY*CustomFlaerdY+CustomFlaerdZ*CustomFlaerdZ)^0.5
        limit=-limit
        if limit<er then
          er=limit/er
          CustomFlaredX=CustomFlaredX*er
          CustomFlaredY=CustomFlaredY*er
          CustomFlaredZ=CustomFlaredZ*er
        end
      end
      CustomFlareCX=CustomFlaredX+CustomFlareXX
      CustomFlareCY=CustomFlaredY+CustomFlareYY
      CustomFlareCZ=CustomFlaredZ+CustomFlareZZ
    end
  end

  CustomFlareColor = col
  CustomFlareW,CustomFlareH=obj.getpixel()
  CustomFlareMode=1+3*obj.track1
  infre=nil
  ko=nil
  limit=nil

@Iris
--track0:Shape,1,10,1,1
--track1:Count,1,100,4,1
--track2:Size(%),0,5000,30
--track3:Strength,0,100,50
--dialog:SizePlus(%),local dsize=50;SuccessiveEnlarge/chk,local biger=0;StrPlus(%),local dalp=5;BaseColor/chk,local basechk=1;Color/col,local col=0xccccff;ColorPlus(%),local dcol=5;Pos(%),local PP={0,5};PosOffset,local OFSET={0,0,0};Scatter(%),local SIG={100,25};Spin,local KAITEN={0,0};Match anchor/chk,local acr=0;Blur,local blur=10;Blink,local blink=0.2;RandomSeed,local seed=0;CamStrength,local infre=0;
  if oncamera==0 then
    infre=1
  else
    if infre==nil then
      infre=0
    end
    infre=infre*0.01
    infre=1/(1+(d-cd)/cd*infre)*tr3
    if infre<=0 then
      infre=0
    end
  end

  obj.copybuffer("tmp","obj")
  obj.setoption("drawtarget","tempbuffer")
  obj.setoption("blend",CustomFlareMode)
  if basechk==1 then
    col=CustomFlareColor
  end

  local fig  =obj.track0
  local count=obj.track1
  local size =obj.track2*0.01
  local alp  =obj.track3*0.01
  local t=PP[1]*0.01
  local dt=PP[2]
  local sp=SIG[1]*0.01
  local dsp=SIG[2]
  local rot=KAITEN[1]
  local drot=KAITEN[2]*0.5
  OFSET[1]=OFSET[1]*0.01
  OFSET[2]=OFSET[2]*0.01
  OFSET[3]=OFSET[3]*0.01

  obj.load("image",obj.getinfo("script_path").."CF-image\\I"..fig..".png")
  obj.effect("Gradient","color",col,"color2",col,"blend",5)
  obj.effect("Blur","Range",blur)

  local OF=math.floor(obj.time*obj.framerate)
  for i=1,count do

    if dcol>0 then
      obj.load("image",obj.getinfo("script_path").."CF-image\\I"..fig..".png")
      local h,s,v = HSV(col)
      h=math.floor(h+math.floor(3.6*obj.rand(0,dcol,i,seed)))%360
      col = HSV(h,s,v)
      obj.effect("Gradient","color",col,"color2",col,"blend",5)
      obj.effect("Blur","Range",blur)
    end

    local hi=((i-0.5)/count-0.5)*(1+obj.rand(-dsp,dsp,i,1000+seed)*0.01)
    hi=t+hi*sp
    local ox=CustomFlaredX*(hi+obj.rand(-dt,dt,i,2000+seed)*0.005+OFSET[1])
    local oy=CustomFlaredY*(hi+obj.rand(-dt,dt,i,3000+seed)*0.005+OFSET[2])
    local oz=CustomFlaredZ*(hi+obj.rand(-dt,dt,i,4000+seed)*0.005+OFSET[3])

    local zoom=CustomFlaredX*CustomFlaredX+CustomFlaredY*CustomFlaredY+CustomFlaredZ*CustomFlaredZ
    if zoom==0 or biger==0 then
      zoom=1
    else
      zoom=math.sqrt(((CustomFlaredX+ox)*(CustomFlaredX+ox)+(CustomFlaredY+oy)*(CustomFlaredY+oy)+(CustomFlaredZ+oz)*(CustomFlaredZ+oz))/zoom*0.25)
    end

    ox=CustomFlareCX+ox
    oy=CustomFlareCY+oy
    oz=CustomFlareCZ+oz

    zoom=zoom*size*(1-obj.rand(0,dsize,i,5000+seed)*0.01)

    local alpha = obj.rand(0,100,i,OF+seed)/100+(1-blink)
    if alpha>1 then alpha=1 end
    alpha=alp*alpha*obj.rand(100-dalp*0.5,100+dalp*0.5,i,6000+seed)*0.01

    local rz=rot+obj.rand(-drot,drot,i,7000+seed)
    if acr==1 then
      rz=rz+math.deg(math.atan2(CustomFlaredY,CustomFlaredX))
    end
    obj.draw(ox,oy,oz,zoom*infre,alpha*palpha,0,0,rz)

  end

  obj.load("tempbuffer")
  obj.setoption("blend",0)
  infre=nil

