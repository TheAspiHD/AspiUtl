	dy0 = -math.cos(rr+math.pi+math.pi/num)*l
	dx1 =  math.sin(rr+math.pi/2)*ll
	dy1 = -math.cos(rr+math.pi/2)*ll
	for li=0,ln do
		x0 = li*dx0
		y0 = li*dy0
		x1 = x0+dx0
		y1 = y0+dy0
		x2 = x1+dx1
